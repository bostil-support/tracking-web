@model List<IGrouping<Guid, Survey>>

@foreach (var item in Model)
{
    <div class="day">
        <div>
            <span class="date-text bold">
                @item.ElementAt(0).InterventionName
            </span>
            <span class="date">
                Data inserimento: @item.ElementAt(0).ImportDownloadDate.ToString("dd/MM/yyyy")
            </span>
        </div>
        <div>
            @foreach (var survey in item)
            {
                @Html.Partial("_SurveySummary", survey)
            }
        </div>
    </div>
}
